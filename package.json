{
  "name": "xenharmonics",
  "version": "0.0.0",
  "scripts": {
    "installUnsupported": "npm install && npm run installWiringPiUnsupported",
    "installPi": "npm install && npm run installWiringPi",
    "installWiringPiUnsupported": "npm install --no-save node-wiring-pi --ignore-scripts",
    "installWiringPi": "npm install --no-save node-wiring-pi",
    "configPi": "cp ./server/configs/configPi.ts ./server/config.ts",
    "configUnsupported": "cp ./server/configs/configUnsupported.ts ./server/config.ts",
    "buildClient": "ng build",
    "buildServer": "./node_modules/typescript/bin/tsc -p server",
    "build": "npm run buildClient && npm run buildServer",
    "start": "node ./dist/server/index.js",
    "debug": "node ./dist/server/index.js debug"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "^7.0.3",
    "@angular/compiler": "^7.0.3",
    "@angular/core": "^7.0.3",
    "@angular/forms": "^7.0.3",
    "@angular/http": "^7.0.3",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "^7.0.3",
    "@angular/platform-browser-dynamic": "^7.0.3",
    "@angular/router": "^7.0.3",
    "@tsed/common": "^4.32.0",
    "@tsed/core": "^4.32.0",
    "@tsed/servestatic": "^4.32.0",
    "@tsed/swagger": "^4.32.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "hammerjs": "^2.0.8",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "redis": "^3.1.1",
    "rxjs": "^6.0.0",
    "ts-httpexceptions": "^3.1.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.5",
    "@angular/cli": "^7.0.5",
    "@angular/compiler-cli": "^7.0.3",
    "@angular/language-service": "^7.0.3",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.11",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.4",
    "@types/redis": "^2.8.8",
    "@types/swagger-schema-official": "^2.0.12",
    "@types/wiring-pi": "^2.2.3",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^3.1.6"
  }
}
